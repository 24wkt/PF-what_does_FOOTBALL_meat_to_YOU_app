<% if end_user_signed_in? %>
  <%= form_tag(search_path, method: :get) do %>
    <%= select_tag 'model', options_for_select({"User" => "end_user", "Blog" => "blog"}) %>
    <%= select_tag 'method', options_for_select({"完全一致" => "match", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partical"}) %>
    <%= text_field_tag 'content' %>
    <%= submit_tag :search, class: "btn btn-sm btn-secondary" %>
  <% end %>
<% else %>
  <%= form_tag(admin_search_path, method: :get) do %>
    <%= select_tag 'model', options_for_select({"User" => "end_user", "Blog" => "blog"}) %>
    <%= select_tag 'method', options_for_select({"完全一致" => "match", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partical"}) %>
    <%= text_field_tag 'content' %>
    <%= submit_tag :search, class: "btn btn-sm btn-secondary" %>
  <% end %>
<% end %>